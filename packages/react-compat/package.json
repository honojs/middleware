{
  "name": "@hono/react-compat",
  "type": "module",
  "version": "0.0.1",
  "description": "Alias of hono/jsx for replacement of React",
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src --format esm,cjs --dts",
    "publint": "publint",
    "release": "bun run build && bun run test && bun run publint && bun run publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "peerDependencies": {
    "hono": ">=4.5.*"
  },
  "devDependencies": {
    "hono": "^4.5.0",
    "tsup": "^8.0.1"
  }
}