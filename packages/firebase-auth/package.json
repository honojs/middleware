  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "firebase-auth-cloudflare-workers": "^1.0.1"
  },
  "peerDependencies": {
    "hono": "3.*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@types/jest": "^28.1.4",
    "firebase-tools": "^11.4.0",
    "hono": "^3.0.3",
    "jest": "^28.1.2",
    "jest-environment-miniflare": "^2.6.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
